 */
public class EntityAppearanceLifeFormStateField extends Object
{
/**
 *(0) Null
 */
public static final short NULL = 0;

/**
 *(1) Upright, standing still
 */
public static final short UPRIGHTSTANDINGSTILL = 1;

/**
 *(2) Upright, walking
 */
public static final short UPRIGHTWALKING = 2;

/**
 *(3) Upright, running
 */
public static final short UPRIGHTRUNNING = 3;

/**
 *(4) Kneeling
 */
public static final short KNEELING = 4;

/**
 *(5) Prone
 */
public static final short PRONE = 5;

/**
 *(6) Crawling
 */
public static final short CRAWLING = 6;

/**
 *(7) Swimming
 */
public static final short SWIMMING = 7;

/**
 *(8) Parachuting
 */
public static final short PARACHUTING = 8;

/**
 *(9) Jumping
 */
public static final short JUMPING = 9;

/**
 * Returns a string containing the enumeration name which corresponds to an enumeration value,
 * as in <b><code>EntityAppearanceLifeFormStateField.toString (0)</code></b> returns the string "<b><code>NULL</code></b>"
 */

public static String toString(int idNumber) 
{
	switch (idNumber) {
		case 0: return "Null"; 
		case 1: return "Upright, standing still"; 
		case 2: return "Upright, walking"; 
		case 3: return "Upright, running"; 
		case 4: return "Kneeling"; 
		case 5: return "Prone"; 
		case 6: return "Crawling"; 
		case 7: return "Swimming"; 
		case 8: return "Parachuting"; 
		case 9: return "Jumping"; 
		default : return "";
	}
}//end of toString
}//End of class 
